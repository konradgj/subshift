{
  "name": "subshift",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "bin": {
    "subshift": "./dist/bin/subshift.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/bin/subshift.js && npm link",
    "dev": "tsx bin/subshift.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --fix",
    "prettier": "prettier . --write"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  }
}
